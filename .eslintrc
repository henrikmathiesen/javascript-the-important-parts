{
    "extends": "airbnb-base",
    "parserOptions": {
        "ecmaVersion": 6,
        "sourceType": "module"
    },
    "env": {
      "es6": true,
      "browser": true,
      "node": true,
      "jquery": true
    },
    "rules": {
      "no-console": 1,
      "no-debugger": 1,
      "linebreak-style": 0,
      "indent": ["error", 4]
    }
}
