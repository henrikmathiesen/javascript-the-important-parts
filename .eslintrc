{
    "extends": "airbnb-base",
    "parserOptions": {
        "ecmaVersion": 6,
        "sourceType": "module"
    },
    "env": {
      "es6": true,
      "browser": true
    },
    "rules": {
      "no-console": 0,
      "no-debugger": 1,
      "linebreak-style": 0,
      "indent": ["error", 4],
      "no-unused-vars": 0
    }
}
